<!--pages/lawyer/tixian/tixian.wxml-->
<view class="content">
  <view class="Mymoney">
    <view>我的余额</view>
    <view>{{totalBalance*0.01}}元</view>
  </view>
  <view class="draw">
    <view>提现金额</view>
    <input type="text" bindinput="TJmoney" placeholder="请输入提现金额" />
    <view class="tips">温馨提示：每次提现需收取10%的手续费</view>
  </view>
  <view class="tixian" bindtap="tixian">确定</view>
</view>
